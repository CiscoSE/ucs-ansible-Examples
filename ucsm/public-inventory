[GiveMeAName]
FirstUCSDomain     ucsm_host=<SomeIP>
SecondUCSDomain    ucsm_host=<SomeOtherIP>

#These are common variables for the above systems
#The prefix should match the domain above.
[GivMeAName:vars]
